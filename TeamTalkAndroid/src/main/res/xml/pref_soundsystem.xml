<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >
    
    <CheckBoxPreference
		android:key="mute_speakers_on_tx_checkbox"
		android:defaultValue="false"
		android:title="@string/pref_title_mute_speakers"
		android:summary="@string/pref_summary_mute_speakers" />

	<ListPreference
		android:key="audiomode_list"
		android:entries="@array/audiomodeNames"
		android:entryValues="@array/audiomodeValues"
		android:title="@string/pref_title_audiomode_list"
		android:summary="@string/pref_summary_audiomode_list"/>

	<CheckBoxPreference
		android:key="speakerphone_checkbox"
		android:defaultValue="false"
		android:title="@string/pref_title_speakerphone_checkbox"
		android:summary="@string/pref_summary_speakerphone_checkbox" />

	<dk.bearware.gui.SeekBarPreference
	    android:key="mediafilevolume_seekbar"
	    android:defaultValue="100"
	    android:title="@string/pref_title_mediafile_volume"
	    android:summary="@string/pref_summary_mediafile_volume" />

</PreferenceScreen>
